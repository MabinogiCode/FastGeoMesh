<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <PackageId>FastGeoMesh</PackageId>
    <Authors>FastGeo</Authors>
    <Company>FastGeo</Company>
    <Description>Lightweight quad meshing utilities for generic 2.5D prism structures.</Description>
    <RepositoryUrl>https://example.com/fastgeomesh</RepositoryUrl>
    <PackageProjectUrl>https://example.com/fastgeomesh</PackageProjectUrl>
    <PackageTags>mesh;quad;geometry</PackageTags>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="LibTessDotNet" Version="1.1.15" />
  </ItemGroup>
</Project>
